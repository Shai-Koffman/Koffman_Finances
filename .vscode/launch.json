    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Bank file W/Debug",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": ["--bank", ".expenses/bank_march23-march24.xls"]
        },
        {
            "name": "Python Debugger: Visa File with Debug",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/parse_visa_leumi.py",
            "console": "integratedTerminal",
            "args": ["-v", ".expenses/max_march23-march24.xls"]
        },
    ]
}

